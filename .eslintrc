{
  "extends": "@meadow",
  "settings": {
    "react": {
      "version": "16"
    }
  },

  "rules": {
    "consistent-return": 0,
    "no-multi-assign": 0,
    "no-process-env": 0,
    "no-var": 0,
    "object-shorthand": 0,
    "prefer-spread": 0,
    "prefer-template": 0,
    "react/jsx-handler-names": 0,
    "strict": 0,
    "react/no-this-in-sfc": 0,
    "react/display-name": 0
  }
}